{
    "name": "c0py/lumen-react",
    "description": "API-Centric Architecture",
    "keywords": ["api", "laravel", "lumen", "ldap"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.3.*",
        "vlucas/phpdotenv": "~2.2",
        "guzzlehttp/guzzle": "~6.0",
        "adldap2/adldap2-laravel": "2.0.*",
        "jenssegers/agent": "^2.3",
        "predis/predis": "^1.1",
        "illuminate/redis": "5.2.*",
        "illuminate/mail": "5.2.*",
        "ramsey/uuid": "^3.5",
        "elasticsearch/elasticsearch": "^2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0",
        "codeception/codeception": "2.1.x-dev",
        "flow/jsonpath": "~0.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
          "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    }
}
